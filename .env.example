# 괌세이브카드 환경 설정 파일
# .env 파일로 복사하여 사용하세요

# 서버 설정
PORT=3000
NODE_ENV=development

# 데이터베이스 설정
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_password_here
DB_NAME=guam_savecard

# 세션 보안 키 (운영환경에서는 반드시 변경하세요)
SESSION_SECRET=guam-savecard-secret-key-change-this-in-production

# 기타 설정
TIMEZONE=Asia/Seoul
